{
  "name": "Kanban local",
  "version": "0.0.0",
  "main": "dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:electron": "tsc --outDir ./dist src/electron/main.ts src/electron/preload.ts && ng build --base-href ./ && cross-env ENV=production pnpx electron .",
